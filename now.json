{
  "version": 2,
  "name": "tunedtoned-mirror",
  "alias": "tunedtoned-mirror-example-url.now.sh",
  "builds": [{
    "src": "index.js",
    "use": "@now/node-server"
  }],
  "engines": {
    "node": "8.x"
  },
  "routes": [{
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
      "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
    },
    "src": "/.*",
    "dest": "/index.js"
  }]
}
